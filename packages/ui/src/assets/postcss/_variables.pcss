   --spacing-3xs: clamp(0.44rem, 0.5rem + 0.07vw, 0.5rem); /* 7px - 8px */
   --spacing-2xs: clamp(0.57rem, 0.69rem + 0.13vw, 0.69rem); /* 9px - 11px */
