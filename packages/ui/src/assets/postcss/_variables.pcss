   --spacing-2xs-xs: clamp(0.5rem, calc(0.39rem + 0.45vw), 0.94rem);
   --spacing-xs-s: clamp(0.75rem, calc(0.63rem + 0.52vw), 1.25rem);
   --spacing-s-m: clamp(1rem, calc(0.79rem + 0.91vw), 1.88rem);
   --spacing-m-l: clamp(1.5rem, calc(1.26rem + 1.04vw), 2.5rem);
   --spacing-l-xl: clamp(2rem, calc(1.58rem + 1.81vw), 3.75rem);
   --spacing-xl-2xl: clamp(3rem, calc(2.51rem + 2.07vw), 5rem);
   --spacing-2xl-3xl: clamp(4rem, calc(3.15rem + 3.62vw), 7.5rem);
   --spacing-2xl-xl: clamp(2.1875rem, 1.8538rem + 1.4239vw, 3.5625rem);
 
   /* Two-up pairs */
   --spacing-3xs-xs: clamp(0.25rem, calc(0.08rem + 0.71vw), 0.94rem);
   --spacing-2xs-s: clamp(0.5rem, calc(0.32rem + 0.78vw), 1.25rem);
   --spacing-xs-m: clamp(0.75rem, calc(0.48rem + 1.17vw), 1.88rem);
   --spacing-s-l: clamp(1rem, calc(0.64rem + 1.55vw), 2.5rem);
   --spacing-m-xl: clamp(1.5rem, calc(0.95rem + 2.33vw), 3.75rem);
   --spacing-l-2xl: clamp(2rem, calc(1.27rem + 3.11vw), 5rem);
   --spacing-xl-3xl: clamp(3rem, calc(1.91rem + 4.66vw), 7.5rem);

   /* Custom pairs */
   --spacing-5xl-3xl: clamp(3.8125rem, 2.6899rem + 4.7896vw, 8.4375rem);
   --spacing-5xl-md: clamp(1.25rem, -0.4945rem + 7.4434vw, 8.4375rem);
   --spacing-5xl-xl: clamp(2.1875rem, 0.6705rem + 6.4725vw, 8.4375rem);
   /**
   * @tokens-end
   */

  /* ---- BASE ---- */
  /* ---- Base-mobile ---- */
  --base-font-size-mobile: 1rem; /* 16px */
  --base-line-height-mobile: 1.2; /* 16 * 1.2 = 19.2px */
  /* ---- Base-desktop ---- */
  --base-font-size-desktop: 1.25rem; /* 20px */
  --base-line-height-desktop: 1.2; /* 20px * 1.2 = 24px */
  /* --source-code-pro-font: '', sans-serif; */
  --base-font-family: var(--trenda-font, sans-serif);
  /* --secondary-font: var(--cobane-font); */

  /* Font weights */
  --font-weight-normal: 400;
  --font-weight-medium: 500;
  --font-weight-semibold: 600;
  --font-weight-bold: 700;
  --base-font-size: var(--font-size-md, 1rem);

  /* ---- LAYOUT ---- */
  /* --------------------------------- */
  /* Set your wrapper width */
  /* Configuration is in layout.pcss */
  --wrapper-width: calc(100vw - 20%);
  --spacing-base: var(--spacing-md);
  /* Layout spacing */
  --layout-spacing-block: var(--spacing-base);
  --layout-spacing-inline: var(--spacing-base);

  /* Header */
  --header-padding-block: clamp(2.1875rem, 1.5655rem + 2.6537vw, 4.75rem);
  --header-logo-height: clamp(2rem, 1.6359rem + 1.5534vw, 3.5rem);
  --header-height-offset: calc(calc(var(--header-padding-block) * 2) + var(--header-logo-height));


