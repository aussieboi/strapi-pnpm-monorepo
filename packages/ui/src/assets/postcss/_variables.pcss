  * @tokens-end
  */

/* ---- FONT FAMILIY ---- */
--primary-font: 'Trenda', -apple-system, blinkmacsystemfont, 'Segoe UI', roboto, oxygen,
ubuntu, cantarell, 'Fira Sans', 'Droid Sans', 'Helvetica Neue', sans-serif;
 

  /* ---- Font Size ---- */
  --font-size-xs: clamp(0.57rem, 0.53rem + 0.15vw, 0.71rem); /* 9px -> 11.26px */
  --font-size-sm: clamp(0.75rem, 0.71rem + 0.2vw, 0.94rem); /* 12px -> 15px */
  --font-size-md: clamp(1rem, 0.94rem + 0.26vw, 1.25rem); /* 16px -> 20px */
  --font-size-lg: clamp(1.34rem, 1.26rem + 0.35vw, 1.67rem); /* 21.33px -> 26.66px */
  --font-size-xl: clamp(1.78rem, 1.67rem + 0.47vw, 2.23rem); /* 28.43px -> 35.54px */
  --font-size-2xl: clamp(2.37rem, 2.23rem + 0.62vw, 2.97rem); /* 37.9px -> 47.37px */
  --font-size-3xl: clamp(3.16rem, 2.97rem + 0.82vw, 3.95rem); /* 50.52px -> 63.15px */
  --font-size-4xl: clamp(4.21rem, 3.96rem + 1.09vw, 5.27rem); /* 67.34px -> 84.17px */
  --font-size-5xl: clamp(5.61rem, 5.27rem + 1.46vw, 7.02rem); /* 89.76px -> 112.2px */
  --font-size-6xl: clamp(7.48rem, 7.03rem + 1.94vw, 9.35rem); /* 119.66px -> 149.57px */
  --font-size-7xl: clamp(9.97rem, 9.37rem + 2.59vw, 12.47rem); /* 159.5px -> 199.38px */
  

