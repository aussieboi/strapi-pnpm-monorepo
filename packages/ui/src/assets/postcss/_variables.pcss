   --spacing-mobile-sm: 15px;
   --spacing-mobile-md: 20px;
   --spacing-mobile-lg: 26px;
   --spacing-mobile-xl: 35px;
   --spacing-mobile-2xl: 46px;
   --spacing-mobile-3xl: 61px;
   --spacing-mobile-4xl: 81px;
   --spacing-mobile-5xl: 108px;
   --spacing-mobile-6xl: 144px;
   --spacing-mobile-7xl: 192px;
   --spacing-mobile-8xl: 256px;
   --spacing-mobile-9xl: 341px;
   --spacing-mobile-10xl: 454px;
 
   /* Desktop Spacings */
