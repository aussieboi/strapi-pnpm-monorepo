   --spacing-md: clamp(1.25rem, 1.44rem + 0.26vw, 1.5rem); /* 20px - 24px */
   --spacing-lg: clamp(1.63rem, 1.92rem + 0.39vw, 2rem); /* 26px - 32px */
   --spacing-xl: clamp(2.19rem, 2.58rem + 0.52vw, 2.69rem); /* 35px - 43px */
   --spacing-2xl: clamp(2.88rem, 3.42rem + 0.72vw, 3.57rem); /* 46px - 57px */
   --spacing-3xl: clamp(3.82rem, 4.56rem + 0.98vw, 4.75rem); /* 61px - 76px */
